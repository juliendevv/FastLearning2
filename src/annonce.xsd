<?xml version="1.0" encoding="UTF-8"?>
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="annonce">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="Nom" type="xs:string" />
          <xs:element name="Prenom" type="xs:string" />
          <xs:element name="DateDebut" type="xs:date" />
          <xs:element name="DateFin" type="xs:date" />
          <xs:element name="Email" type="xs:string" />
          <xs:element name="Tarif" type="xs:integer" />
          <xs:element name="NombreCours" type="xs:integer" />
          <xs:element name="Matiere" type="xs:string"/>
          <xs:element name="Niveau" type="xs:string" />
          <xs:element name="Description" type="xs:string" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>
  </xs:schema> 